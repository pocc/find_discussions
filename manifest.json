{
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["build/main.js"],
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
        "build/hn.js",
        "build/reddit.js",
        "build/stackexchange.js",
        "build/settings.js",
        "build/utils.js"
    ],
    "manifest_version": 2,
    "author": "Ross Jacobs",
    "name": "Find Discussions",
    "description": "Browser extension to find discussions of your current page",
    "version": "0.0.1"
  }